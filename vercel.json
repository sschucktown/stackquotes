{
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@5.0.0"
    }
  },
  "installCommand": "pnpm install --filter ./apps/api... --ignore-scripts",
  "buildCommand": "pnpm --filter @stackquotes/api run build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
