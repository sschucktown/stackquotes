{
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@5.0.0"
    }
  },
  "installCommand": "pnpm install --no-frozen-lockfile --filter ./apps/api...",
  "buildCommand": "pnpm --filter @stackquotes/api run build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "outputDirectory": "."
}
