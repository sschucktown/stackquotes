{
  "functions": {
    "apps/api/src/index.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "installCommand": "pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm run vercel-build",
  "outputDirectory": "apps/web/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/api/src/index.ts"
    }
  ]
}
