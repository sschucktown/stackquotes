{
  "version": 2,
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm vercel-build",
  "outputDirectory": "apps/web/dist",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/[[...stackquotes]]" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
