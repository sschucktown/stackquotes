{
  "name": "stackquotes",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "turbo run dev --parallel --no-cache",
    "dev:web": "pnpm --filter @stackquotes/web dev",
    "dev:api": "pnpm --filter @stackquotes/api dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "vercel-build": "turbo run build"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "turbo": "^2.5.8",
    "concurrently": "^8.2.0",
    "typescript": "^5.5.4",
    "@types/node": "^20.14.9"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "@hono/node-server": "^1.19.5",
    "hono": "^4.9.10"
  }
}
