{
  "name": "@stackquotes/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm",
    "dev": "tsup src/index.ts --format esm --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc -p tsconfig.build.json --noEmit"
  },
  "dependencies": {
    "@stackquotes/config": "workspace:*",
    "@stackquotes/types": "workspace:*",
    "@supabase/supabase-js": "^2.45.3"
  },
  "devDependencies": {
    "tsup": "^8.0.1"
  }
}
