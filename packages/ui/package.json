{
  "name": "@stackquotes/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --watch --format esm --dts",
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "typecheck": "vue-tsc -p tsconfig.vue.json --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "peerDependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "vue-tsc": "^2.0.28"
  }
}
